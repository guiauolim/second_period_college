

import java.util.Scanner;



public class Cliente extends Main {
	String n = "Guilherme Augusto";
	String e = "guiauolim@gmail.com";	
	String nome;
	String email;
	String salvar;
	int idade;
	int valor;
	
	public String nome () {
		Scanner leitor = new Scanner(System.in);
		System.out.print(" Digite seu nome: ");
		nome = leitor.next();
		leitor.close();
		return nome;
		}
	
	public String email () {
		Scanner leitor = new Scanner(System.in); 
		System.out.print(" Digite o seu email: ");
		email = leitor.next();
		leitor.close();
		return email;
	}
	
	public int idade (int valor) {
		System.out.println(valor);
		return idade;
		
	}
	public String salvar() {
		if ((nome.equals(n)) && (email.equals(e)))  {
			System.out.println(" Usuário cadastrado! ");	
		} else {
			System.out.println(" Não foi possível continuar a operação! ");
		}
		
		return salvar;
	}
	
	} 
	
	
	


