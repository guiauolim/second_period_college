
public class MediaNota {

	public static void main(String[] args) {
		int media1 = 0;
		int media2 = 0;
		int media3 = 0;
		int soma = 0;
		int a = 8;
		int b = 9;
		int c = 7;
		int d = 4;
		int e = 5;
		int f = 6;
		
		media1 = ((a + b + c)/3);
		System.out.print(" A primeira média é: " + media1);
		
		media2 = ((d + e + f)/3);
		System.out.print(" A segunda média é " + media2);
		
		soma = media1 + media2;
		System.out.println(" A soma das médias: " + soma);
		
		media3 = (soma/2);
		System.out.print(" A média do número acima é: " + media3);
	
	
	
	
	}
	
	
	
	
	
	

}
